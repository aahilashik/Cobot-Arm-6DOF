<launch>

  <arg name="model" />
  <arg name="gui" default="False" />
  <param name="robot_description" textfile="$(find cobot_arm_urdf)/urdf/cobot_arm_urdf.urdf" />

  <node if="$(arg gui)" name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find cobot_arm_urdf)/config/view_robot.rviz" />

</launch>
